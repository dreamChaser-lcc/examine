{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "dev": "npm run start:dev",
    "start:dev": "cross-env NODE_ENV=dev webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "build": "cross-env Umi_UI=none cross-env UMI_BUILD_THREAD_WORKERS=0 cross-env NODE_OPTIONS=\"--max-old-space-size=1024\" umi build",
    "analyze:build": "cross-env ANALYZE=1 umi build",
    "deploy":"node ./client/deploy.js",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-descriptions": "^1.7.8",
    "@ant-design/pro-layout": "^6.5.0",
    "@ant-design/pro-table": "^2.36.0",
    "@antv/g2": "^4.1.16",
    "@umijs/preset-react": "^1.8.30",
    "@umijs/renderer-react": "3.x",
    "ahooks": "^3.5.0",
    "animate.css": "^4.1.1",
    "antd": "^4.15.4",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "echarts": "^5.3.1",
    "echarts-for-react": "^3.0.2",
    "http-proxy-middleware": "^2.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "react-activation": "^0.9.4",
    "react-router-config": "^5.1.1",
    "react-sortablejs": "^6.0.0",
    "react-transition-group": "^4.4.2",
    "react-window": "^1.8.7",
    "sortablejs": "^1.14.0",
    "ssh2": "^1.11.0",
    "umi": "3.4.7"
  },
  "devDependencies": {
    "@types/echarts": "^4.9.13",
    "@types/lodash": "^4.14.168",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-transition-group": "^4.4.4",
    "@types/react-window": "^1.8.5",
    "@types/sortablejs": "^1.10.7",
    "@umijs/test": "^3.4.7",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "webpack-dev-server": "^4.0.0",
    "yorkie": "^2.0.0"
  }
}
